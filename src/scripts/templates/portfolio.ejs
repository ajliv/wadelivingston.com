<div class="wlp-photos">
    <% _.forEach(photosets, function (photoset) { %>
    <div class="photoset">
        <a href="#portfolio/<%= _s.slugify(photoset.title) %>">
            <img src="<%= photoset.cover.large.src %>" alt="<%= photoset.title %>">
            <span class="photoset-info">
                <h2 class="photoset-title"><%= photoset.title %></h2>
            </span>
        </a>
    </div>
    <% }); %>
</div>